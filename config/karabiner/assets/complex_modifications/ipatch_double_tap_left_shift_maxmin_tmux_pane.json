{
  "title": "ipatch, ke, local complex modifications",
    "maintainer": [
    "ipatch"
  ],

  "rules": [
    { "description": "tap left_shift to send shift+option+control+f1",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_shift",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_alone": [
            {
              "key_code": "f1",
              "modifiers": [
                "left_shift",
                "left_control",
                "left_option"
              ]
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "left_shift"
            }
          ],
          "parameters": {
            "basic.to_if_held_down_threshold_milliseconds": 200
          }
        }
      ]
    }
  ]
}
