{ 
  "title": "ipatch, CHANGEME, vim snippet for scaffolding karabiner complex mod",
  "maintainer": [
    "github.com/ipatch"
  ],
  "rules": [
    {
      "description": "ipatch, CHANGEME, listing of key mods",
      "manipulators": [
        {
          "type": "basic",
          "TODO": "from key must be a JSON object NOT arrary",
          "from": 
          {
            "key_code": "INSERT KEYCODE",
            "modifiers": {
              "optional": [
                "any"
                ]
            }
          }
          ,
          "to": [
            {
              "key_code": "INSERT KEYCODE"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "INSERT KEYCODE",
              "hold_down_milliseconds": "EX 500"
            }
          ]
        }
      ]
    }
  ]
}
